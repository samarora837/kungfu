<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.VO.AssignRankVO" table="UpgradeRank">
  
   <id name="assign_rank_id" type="java.lang.Integer">
            <column name="ASSIGN_RANK_ID" />
            <generator class="identity" />
   </id>
 
  <property name="assign_rank_date" type="java.util.Date">
   	<column name="ASSIGN_RANK_DATE" />
  </property>
  
<property name="student_NUM">
   	<column name="ASSIGN_STUDENT_NUM" />
  </property>
  
  <property name="rankName" >
   	<column name="ASSIGN_RANK_NAME" />
  </property>
  
  </class>
</hibernate-mapping>